<h1>User info</h1>

<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
    <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>

<% @books.each do |book| %>
<div>

  <p>Title</p>
  <span><%= link_to book.title, book_path(book.id) %></span>
</div>
<% end %>